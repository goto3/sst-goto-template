{
  "eslint.enable": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "eslint.validate": [
    "javascript",
    "typescript",
  ],
  "files.eol": "\n",
  "files.exclude": {
    "**/.build": true,
    "**/node_modules": true
  },
  "search.exclude": {
    "**/.sst": true,
    "**/coverage/**": true,
  }
}