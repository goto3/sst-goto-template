{
  "extends": "@tsconfig/node18/tsconfig.json",
  "exclude": [
    "node_modules"
  ],
  "compilerOptions": {
    "module": "esnext",
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "@core/*": [
        "../core/*"
      ],
      "@Services/*": [
        "../core/services/*"
      ],
      "@Errors/*": [
        "../core/services/*"
      ],
    }
  }
}